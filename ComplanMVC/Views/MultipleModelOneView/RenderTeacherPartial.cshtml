@using ComplanMVC.Models;
@model IEnumerable<ComplanMVC.Models.Teacher>

<table class="table table-bordered" style="width: 800px;">
    <tr style="background-color: #a94442; font-weight: bold; color: white;">
        <th>Id</th>
        <th>Code</th>
        <th>Name</th>
    </tr>
    @foreach (Teacher T in Model)
    {
        <tr>
            <td>@T.TeacherId</td>
            <td>@T.Code</td>
            <td>@T.Name</td>
        </tr>
    }
</table>
