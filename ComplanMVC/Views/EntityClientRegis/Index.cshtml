@model ComplanMVC.Models.ClientRegisClass

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/CompMaster.cshtml";
}


<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">EF-CLient Regis</h1>
    </div>
</div>
<div class="row">
    <div class="panel-body">
        <div class="row">
            <table class="table table-bordered" style="width: 600;">
                <tr>
                    <th>SLno</th>
                    <th>Client Name</th>
                    <th>Client Code</th>
                </tr>
                
                @foreach (ComplanMVC.Models.ClientRegisClass obt in ViewBag.CLientMasterlist)
                {
                    <tr>
                        <td>1</td>
                        <td>@obt.ClientName</td>
                        <td>@obt.ClientCode</td>
                    </tr>
                }
            </table>
        </div>
    </div>
</div>
